{
    "swagger": "2.0",
    "info": {
        "description": "This is a covid summary API server.",
        "title": "LMWN Assesment Covid Summary API",
        "contact": {
            "name": "Kuranasaki",
            "email": "kongphop.c@kuranasaki.work"
        },
        "license": {
